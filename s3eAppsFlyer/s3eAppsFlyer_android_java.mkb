#!/usr/bin/env mkb
# Builder mkb file for the java portion of the s3eAppsFlyer extension on android
platform JAVA

files
{
    (source/android)
    s3eAppsFlyer.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"
librarypath "lib/android/AF-Android-SDK-v2.3.1.9.jar"

option output-name=lib/android/s3eAppsFlyer.jar
